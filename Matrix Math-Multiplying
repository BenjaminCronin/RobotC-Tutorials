#include <stdio.h>

int matARows,
  matACols,
  matBRows,
  matBCols;

float matA[2][2],
  matB[2][2],
  cellProd = 0,
  matFin[2][2],
  *matProd;

float matrixMult {
  matARows = 2;
  matACols = 2;
  matBRows = 2;
  matBCols = 2;
  for(int i = 0; i < matARows; i++) {
    for(int k = 0; k < matBCols; k++) {
      matFin[i][k] = 0;
    }
  }

  matA[0][0] = 1;
  matA[0][1] = 2;
  matA[1][0] = 3;
  matA[1][1] = 4;

  matB[0][0] = 5;
  matB[0][1] = 6;
  matB[1][0] = 7;
  matB[1][1] = 8;

  if(matACols == matBRows) {
    for(int row = 0; row < matARows; row++) {
      for(int col = 0; col < matBCols; col++) {
        for(int cell = 0; cell < matACols; cell++) {
          cellProd += matA[row][cell] * matB[cell][col];
        }
        matFin[row][col] = cellProd;
        cellProd = 0;
        printf("%f  ", matFin[row][col]);
      }
      printf("\n");
    }
  }

  else {
    printf("Error!\n");
    printf("Incompatible Dimensions!\n");
  }
  return matProd;
}
